import java.io.File;
import java.io.FileNotFoundException;
import java.util.Scanner;

public class SelectGoodies {

	public static void main(String[] args) throws FileNotFoundException {
		File readFile = new File("C:\\Users\\user\\Desktop\\sample_input.txt");
		Scanner scanner = new Scanner(readFile);
		String employeeString = scanner.nextLine();
		String[] totalEmployees = employeeString.split(":");

		System.out.println(totalEmployees[1]);

		while (scanner.hasNextLine()) {
			String goodies = scanner.nextLine();
			if (!goodies.equals("Goodies and Prices:")) {

				
				String[] goodiePrice = goodies.split(":");
				System.out.println(goodiePrice[-1]);
				

				
			}

		}

	}

}
