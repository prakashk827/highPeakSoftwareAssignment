import java.io.File;
import java.io.FileNotFoundException;
import java.util.ArrayList;
import java.util.Random;
import java.util.Scanner;

public class SelectGoodies {

	public static void main(String[] args) throws FileNotFoundException {
		File readFile = new File("C:\\Users\\user\\Desktop\\sample_input.txt");
		Scanner scanner = new Scanner(readFile);
		String employeeString = scanner.nextLine();
		String[] totalEmployees = employeeString.split(":");

		System.out.println(totalEmployees[1]);

		while (scanner.hasNextLine()) {
			String goodies = scanner.nextLine();
			Random rand = new Random();
			if (!goodies.equals("Goodies and Prices:")) {
				ArrayList<String> selectedGoodies = new ArrayList<String>();
				ArrayList<String> goodiesList = new ArrayList<String>();

				goodiesList.add(scanner.nextLine());

				for (int i = 0; i < totalEmployees.length; i++) {
					int randNum = rand.nextInt(goodiesList.size());
					selectedGoodies.add(goodiesList.get(randNum));

				}

				/*
				 * String[] goodiePrice = goodies.split(":");
				 * System.out.println(goodiePrice[goodiePrice.length-1]);
				 */
			}
			
			System.out.println(selectedGoodies);

		}

	}

}
